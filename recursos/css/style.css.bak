*{
	margin:0;
	padding: 0;
	box-sizing: border-box;
}

#global {
    
    overflow-y: scroll;
}
#mensajes {
    height: auto;
}
.texto {
    padding:4px;
    background:#fff;
}

#moroso {
	background:transparent;
	border:none;
}

main div h1 {
    border: 1px solid #fff;
    border-radius: 20px;
    background-color: #0c14adad;
    color: #fff;
    font-family: 'Agency FB',arial;
}

#global {
  display: table;
  width: 100%;
  height: 100vh;
  background: url(../img/img3.jpg) top center fixed;
  background-size: cover;
  padding-right: 0px;
  padding-left: 0px;
}


.tm-main {
  background: rgba(0, 0, 0, 0.8);
  
  width: 100%;
  
}

#propi{
	padding: 80px 100px 25px;
}